import socket

def domain_to_ip():
    # User se domain name input lenge
    domain = input("\nEnter the domain name (e.g., google.com): ").strip()
    
    try:
        # Domain ki IP resolve karenge
        ip_address = socket.gethostbyname(domain)
        print(f"\nResolved IP address of {domain} is: {ip_address}\n")
    except socket.gaierror:
        # Agar domain galat hua ya resolve nahi hua
        print("\nError: Unable to resolve domain. Check the domain name.\n")
